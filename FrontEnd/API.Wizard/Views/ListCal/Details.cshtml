@model API.Wizard.Models.ListCal

@{
    ViewData["Title"] = "Details";
}

<h1>Detalle</h1>

<div>
    <h4>Lista de Cálculo</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombre de Cálculo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NombreCalculo)
        </dd>
        <dt class="col-sm-2">
            Cliente
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdClieNavigation.Nombre)
        </dd>
        <dt class="col-sm-2">
            Persona
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdPerNavigation.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdCalculo">Editar</a> |
    <a asp-action="Index">Volver a la Lista</a>
</div>
