@model API.Wizard.Models.CalculoMateri

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>¿Seguro que desea eliminar el cálculo de material</h3>
<div>
    <h4>Cálculo de Materiales</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nombre Cálculo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCalculoNavigation.NombreCalculo)
        </dd>
        <dt class="col-sm-2">
            Material
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdMaterialNavigation.NombreMaterial)
        </dd>
        <dt class="col-sm-2">
            ID Paredes
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdMedParedesNavigation.IdMedParedes)
        </dd>
        <dt class="col-sm-2">
            Total Cálculo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TotalCalculo)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdCalMateri" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la Lista</a>
    </form>
</div>
