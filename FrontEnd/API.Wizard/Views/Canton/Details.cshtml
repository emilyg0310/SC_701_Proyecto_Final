@model API.Wizard.Models.Canton

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Cantón</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Código Provincia
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NombreCanton)
        </dd>
        <dt class="col-sm-2">
            Nombre de Cantón
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CodigoProvinciaNavigation.NombreProvincia)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.CodigoCanton">Editar</a> |
    <a asp-action="Index">Volver a la Lista</a>
</div>
